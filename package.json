{
  "dependencies": {
    "@univmarcomm/rochester-design-tokens": "^0.3.0"
  },
  "devDependencies": {
    "sass": "^1.83.4"
  },
  "scripts": {
    "compile_components": "sass --load-path=node_modules/@univmarcomm/rochester-design-tokens/tokens components/components.scss:components/components.css",
    "compile_custom": "sass --load-path=node_modules/@univmarcomm/rochester-design-tokens/tokens custom/custom.scss:custom/custom.css",
    "compile_menu": "sass --load-path=node_modules/@univmarcomm/rochester-design-tokens/tokens menu/menu.scss:menu/menu.css",
    "compile_newsroom": "sass --load-path=node_modules/@univmarcomm/rochester-design-tokens/tokens newsroom/newsroom.scss:newsroom/newsroom.css",
    "upload_components": "php components/upload_components.php",
    "upload_custom": "php custom/upload_custom.php",
    "upload_menu": "php menu/upload_menu.php",
    "upload_newsroom": "php newsroom/upload_newsroom.php",
    "compile_upload_components": "npm run compile_components && npm run upload_components",
    "compile_upload_custom": "npm run compile_custom && npm run upload_custom",
    "compile_upload_menu": "npm run compile_menu && npm run upload_menu",
    "compile_upload_newsroom": "npm run compile_newsroom && npm run upload_newsroom",
    "publish_components": "php components/publish_components.php",
    "publish_custom": "php custom/publish_custom.php",
    "publish_menu": "php menu/publish_menu.php",
    "publish_newsroom": "php newsroom/publish_newsroom.php"
  }
}
